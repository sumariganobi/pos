# Database
# Local: postgresql://user:password@localhost:5432/pos_db
# Vercel: Akan otomatis di-set dari Vercel Postgres
DATABASE_URL="postgresql://user:password@localhost:5432/pos_db?schema=public"

# NextAuth
# Local: http://localhost:3000
# Production: https://your-app.vercel.app (ganti dengan URL Vercel Anda)
NEXTAUTH_URL="http://localhost:3000"

# Generate dengan: openssl rand -base64 32
# Atau: https://generate-secret.vercel.app/32
NEXTAUTH_SECRET="change-this-to-random-secret-key-min-32-characters"

# App
NEXT_PUBLIC_APP_NAME="POS System"
